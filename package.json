{
  "name": "modern-spa-boilerplate",
  "version": "0.2.0",
  "description": "Modern Boilerplate for Single Page Applications",
  "main": "index.html",
  "scripts": {
    "lint": "gulp js:lint && gulp css:lint",
    "autofix": "gulp js:format && gulp css:format",
    "test": "ava",
    "test:watch": "ava --watch",
    "release": "release-it --github.release --npm.publish --non-interactive",
    "release:minor": "release-it --github.release --npm.publish --non-interactive --increment minor",
    "release:major": "release-it --github.release --npm.publish --non-interactive --increment major"
  },
  "keywords": [
    "boilerplate",
    "es6",
    "jspm",
    "babel",
    "postcss"
  ],
  "repository": "https://github.com/sebastian-software/modern-spa-boilerplate.git",
  "author": "Sebastian Werner",
  "license": "Apache-2.0",
  "dependencies": {
    "async": "^2.0.0-rc.2",
    "autoprefixer": "^6.3.6",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.8.0",
    "browser-sync": "^2.12.5",
    "cssstats": "^3.0.0-beta.1",
    "del": "^2.2.0",
    "doiuse": "^2.3.0",
    "gulp": "^3.9.1",
    "gulp-changed": "^1.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^2.0.0",
    "gulp-postcss": "^6.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "gulp-vuesplit": "^0.4.1",
    "html-minifier": "^2.1.2",
    "immutable-css": "^1.1.1",
    "jspm": "^0.17.0-beta.14",
    "load-plugins": "^2.1.0",
    "node-notifier": "^4.5.0",
    "postcss-advanced-variables": "^1.2.2",
    "postcss-assets": "^4.1.0",
    "postcss-calc": "^5.2.1",
    "postcss-color-function": "^2.0.1",
    "postcss-color-hex-alpha": "^2.0.0",
    "postcss-csso": "^1.1.1",
    "postcss-discard-comments": "^2.0.4",
    "postcss-easings": "^0.3.0",
    "postcss-extend": "^1.0.1",
    "postcss-font-system": "0.0.2",
    "postcss-gradient-transparency-fix": "^1.0.2",
    "postcss-import": "git+https://github.com/sebastian-software/postcss-import.git",
    "postcss-layout-selector": "^0.2.5",
    "postcss-load-plugins": "^1.0.1",
    "postcss-modules": "^0.4.1",
    "postcss-nested": "^1.0.0",
    "postcss-reporter": "^1.3.3",
    "postcss-sassy-mixins": "^2.0.0",
    "postcss-will-change": "^1.1.0",
    "posthtml": "^0.8.7",
    "posthtml-css-modules": "^0.1.0",
    "require-dir": "^0.3.0",
    "s15e-javascript": "^0.1.17",
    "vue-template-validator": "^1.1.5"
  },
  "jspm": {
    "name": "app",
    "directories": {
      "packages": "jspm_packages"
    },
    "configFiles": {
      "jspm:browser": "jspm.browser.js",
      "jspm": "jspm.config.js"
    },
    "dependencies": {
      "lodash": "npm:lodash@^4.11.0",
      "normalize.css": "github:necolas/normalize.css@^4.0.0",
      "vue": "npm:vue@^1.0.18"
    },
    "devDependencies": {
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.8"
    },
    "peerDependencies": {
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "module": "github:jspm/nodelibs-module@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha"
    },
    "overrides": {
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      }
    }
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "gulp-stylelint": "^2.0.2",
    "stylefmt": "^3.2.1",
    "stylelint": "^6.2.2",
    "stylelint-config-standard": "^7.0.0"
  }
}
