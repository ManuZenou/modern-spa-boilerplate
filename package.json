{
  "name": "modern-spa-boilerplate",
  "version": "1.0.0",
  "description": "Modern Boilerplate for Single Page Applications",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --cache *.js src/components/*.js",
    "autofix": "eslint --cache --fix *.js src/components/*.js"
  },
  "keywords": [
    "boilerplate",
    "es6",
    "jspm",
    "babel",
    "postcss"
  ],
  "repository": "https://github.com/sebastian-software/modern-spa-boilerplate.git",
  "author": "Sebastian Werner",
  "license": "Apache-2.0",
  "dependencies": {
    "async": "^2.0.0-rc.2",
    "autoprefixer": "^6.3.3",
    "babel-preset-es2015": "^6.6.0",
    "de-indent": "^1.0.2",
    "gulp": "^3.9.1",
    "gulp-connect": "^3.2.0",
    "gulp-postcss": "^6.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-vuesplit": "^0.1.1",
    "html-minifier": "^1.3.1",
    "jspm": "^0.17.0-beta.11",
    "parse5": "^2.1.5",
    "postcss-import": "^8.0.2",
    "postcss-modules": "^0.4.0",
    "postcss-nested": "^1.0.0",
    "posthtml": "^0.8.3",
    "posthtml-css-modules": "^0.1.0",
    "s15e-javascript": "^0.1.13",
    "vue-template-validator": "^1.1.5"
  },
  "jspm": {
    "name": "app",
    "directories": {
      "packages": "src/jspm_packages"
    },
    "configFiles": {
      "jspm:browser": "src/jspm.browser.js",
      "jspm": "src/jspm.config.js"
    },
    "dependencies": {
      "vue": "npm:vue@^1.0.18"
    },
    "devDependencies": {
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.8"
    },
    "peerDependencies": {
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "module": "github:jspm/nodelibs-module@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha"
    },
    "overrides": {
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      }
    }
  }
}
